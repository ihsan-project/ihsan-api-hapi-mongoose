{
  "name": "maktabah-backend",
  "version": "1.0.0",
  "description": "Maktabah Backend",
  "author": "Mohammed Islam",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "node server",
    "docker": "docker-compose build ; docker-compose up",
    "dev": "npx nodemon server",
    "pretest": "",
    "test": "NODE_ENV=test API_KEY=test_key lab -a @hapi/code -L -I 'server,headers,user' -v",
    "test-list": "lab -v -d",
    "lint": "eslint .",
    "dev-db-setup": "dropdb maktabah-dev ; createdb maktabah-dev && npm run db-migrate && npm run db-seed",
    "db-migrate": "npx knex migrate:latest",
    "db-seed": "npx knex seed:run"
  },
  "dependencies": {
    "@hapi/basic": "^6.0.0",
    "@hapi/boom": "^9.1.2",
    "@hapi/eslint-config-hapi": "^13.0.2",
    "@hapi/eslint-plugin-hapi": "^4.3.6",
    "@hapi/glue": "^8.0.0",
    "@hapi/hapi": "^20.1.3",
    "@hapi/hoek": "^9.2.0",
    "@hapi/joi": "^17.1.1",
    "@hapipal/confidence": "^6.0.1",
    "@hapipal/haute-couture": "^4.1.0",
    "@hapipal/schmervice": "^2.0.0",
    "@hapipal/toys": "^3.1.0",
    "aws-sdk": "^2.912.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "google-auth-library": "^6.1.6",
    "hapi-mongoose": "^2.0.0",
    "hapi-pagination": "^4.0.0",
    "install": "^0.13.0",
    "migrate-mongoose": "^4.0.0",
    "moment": "^2.29.1",
    "mongo-seeding": "^3.6.0",
    "mongoose": "^5.12.10",
    "npm": "^7.15.0"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.3",
    "@hapi/lab": "^24.0.0",
    "@hapipal/hpal": "^3.0.0",
    "@hapipal/hpal-debug": "^2.0.0",
    "faker": "^5.5.3",
    "timekeeper": "^2.2.0"
  }
}
